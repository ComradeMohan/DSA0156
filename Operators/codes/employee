#include <iostream>
using namespcae std;
class Employee {
protected:
    string Emp_name;
    int Emp_id;
    string Address;
    string Mail_id;
    long long Mobile_no;
};

class Programmer : public Employee {
protected:
    double Basic_Pay;
    double DA, HRA, PF, Staff_Club_Fund, Gross_Salary, Net_Salary;
};

class AssistantProfessor : public Employee {
protected:
    double Basic_Pay;
    double DA, HRA, PF, Staff_Club_Fund, Gross_Salary, Net_Salary;
};

class AssociateProfessor : public Employee {
protected:
    double Basic_Pay;
    double DA, HRA, PF, Staff_Club_Fund, Gross_Salary, Net_Salary;
};

class Professor : public Employee {
protected:
    double Basic_Pay;
    double DA, HRA, PF, Staff_Club_Fund, Gross_Salary, Net_Salary;
};